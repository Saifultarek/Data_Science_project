        "def sort_ascending(input_list):\n",
        "    sorted_list = sorted(input_list)\n",
        "    return sorted_list\n",
        "\n",
        "# Given data\n",
        "my_list = [1, 5, 2, 9, 3, 22, 13]\n",
        "\n",
        "sorted_result = sort_ascending(my_list)\n",
        "\n",
        "print(\"Original Given List:\", my_list)\n",
        "print(\"Final Sorted List:\", sorted_result)"
      ]
    }
